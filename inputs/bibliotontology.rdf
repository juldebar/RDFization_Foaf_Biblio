        <res:resource rdf:resource="file:///D:/AYann/Information%2520et%2520documentation/ArticlesPDF/Mes%2520articles/Tremblay%2520et%2520al%25202003%2520Time%2520Budget%2520Common%2520Murrre%2520JexpBiol.pdf"/>
