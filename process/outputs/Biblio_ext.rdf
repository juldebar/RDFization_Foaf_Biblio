  <rdf:Description rdf:about="http://www.ecoscope.org/ontologies/resources/biblioUMRpublication/variabilityofplanktonwithreferencetofishvariabilityinthebenguelacurrentlargemarineecosystemanoverview">
    <dc:identifier>http://zotero.org/users/local/MXbiONP8/items/I4M7CTTD</dc:identifier>
    <dc:description></dc:description>
    <dc:date>2006</dc:date>
    <dc:title>Variability of plankton with reference to fish variability in the benguela current Large Marine Ecosystem – An Overview.</dc:title>
    <rdf:type rdf:resource="http://www.ecoscope.org/ontologies/resources_def/publication"/>
