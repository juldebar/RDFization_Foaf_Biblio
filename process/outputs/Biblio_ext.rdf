  <rdf:Description rdf:about="http://www.ecoscope.org/ontologies/resources/biblioUMRpublication/noteontheyearlycatchesbycountryandgearofthefismfranceivorycoastsenegalmoroccofleetduringtheperiod">
    <dc:creator rdf:resource="http://www.ecoscope.org/ontologies/agents/emmanuelChassot"/>
