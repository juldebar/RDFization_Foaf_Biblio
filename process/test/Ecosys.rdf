  <rdf:Description rdf:about="http://www.ecoscope.org/ontologies/ecosystems/senegalSea">
    <foaf:isPrimaryTopicOf rdf:resource="http://www.ecoscope.org/ontologies/resources/biblioUMRpublication/noteontheyearlycatchesbycountryandgearofthefismfranceivorycoastsenegalmoroccofleetduringtheperiod"/>
