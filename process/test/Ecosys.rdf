    <foaf:isPrimaryTopicOf rdf:resource="http://www.ecoscope.org/ontologies/resources/biblioUMRpublication/asmallpenguinwithabigheartanadaptationfordeeperdiving"/>
    <geographic_objects_def:westBoundLongitude>-100</geographic_objects_def:westBoundLongitude>
