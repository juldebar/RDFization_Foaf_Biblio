    <ecosystems_def:used_data_source rdf:resource="http://www.ecoscope.org/ontologies/resources/dbYugniro"/>
    <ecosystems_def:fishbaseId>http://www.fishbase.org/Summary/SpeciesSummary.php?id=858</ecosystems_def:fishbaseId>
    <ecosystems_def:wormsId>217352</ecosystems_def:wormsId>
    <skos:altLabel xml:lang="es">Tiburón de puntas blancas</skos:altLabel>
