  <rdf:Description rdf:about="http://www.ecoscope.org/ontologies/ecosystems/pacificOcean">
    <geographic_objects_def:prefGeoGraphicObject rdf:resource="http://www.ecoscope.org/ontologies/geographic_objects/wms_pacificocean_1"/>
